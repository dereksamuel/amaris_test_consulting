// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Carrousel {
  id String @id @default(uuid()) @unique
  main_message String
  url String
}

model User {
  id String @id @default(uuid()) @unique
  password String
  email String @unique
  role String
}
